{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "LogCenter": "Trace"
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning",
        "LogCenter": "Information"
      }
    },
    "RemoteLogCenter": {
      "LogLevel": {
        "Default": "Information",
        "LogCenter": "Warning"
      }
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://0.0.0.0:8001"
      }
    }
  },
  "RemoteHubReporter": {
    "Enabled": true,
    "HubUri": "ws://localhost:1635/hubs/logHub",
    "MaxTryCount": 3
  },
  "AllowedHosts": "*"
}
